
@import "variables";
@import "header";



/* Bootstrap XSMALL */

@media screen and (max-width: 767px) {

  #headline-l.make-image div.title {
    font-size: 12px;
    font-weight: @font-top-story-main-title-weight;
    line-height: @font-top-story-main-title-line-height;
    max-width: 813px;
    margin: 0 50px 0 0;   
    padding-bottom: 15px;
  }

  #headline-rt.make-image div.subtitle,
  #headline-rb.make-image div.subtitle  {
    font-size: 14px;
    max-width: 384px;
    margin-left: 30px;  
    padding-bottom: 0;

  }

  #headline-rt.make-image h3 div.violator, 
  #headline-rb.make-image h3 div.violator {
    left: 30px;
    margin-bottom: 0;  
  }

  #headline-rt h2 div.subtitle,
  #headline-rb h2 div.subtitle {
    padding-left: 20px;  
  }


  #headline-rt.make-image img.img-responsive {
    padding-bottom: 1px;
  }




  .ad-unit {
    display: none;
  }





  .events-panel table {
    margin-left: 15%;
  }

  .one-event h4 {
    color: #4A4A4A;
    font-size: 14px;
    font-weight: 300;
    line-height: 1.5;
    margin: 0 15px 0 0;
  }

  .one-event h5 {
    color: #4A4A4A;    
    font-size: 12px;
    font-weight: 300;
    line-height: 1.5;
    margin-top: 12px;
    margin-bottom: 0;
    vertical-align: baseline;
  }

  #camp-event,
  #faire-event {
    border-bottom: 1px solid @background;
  }






  div#shed-heading h5{
    text-align: center;
  }

  #buy-link.row.shed-header h6{
    text-align: center;
  }

  .link-arrow {
    margin-left: 10px;
    vertical-align: top;
  }


  .faire-ad {
    display: none;
  }

 .camp-ad {
    display: none;
  }

}





/* Bootstrap SMALL */

@media screen and (min-width: 768px) and (max-width: 991px) {
 
  .events-panel table {
      margin: 0 10px 0 20px;
    }

  .one-event h4 { 
    color: #4A4A4A;
    font-size: 14px;
    font-weight: 300;
    margin: 0 15px 0 0;
  }

  .one-event h5 {
    color: #4A4A4A;  
    font-size: 12px;
    font-weight: 300;   
    margin-top: 12px;
    margin-bottom: 0;
    vertical-align: baseline;
  }

  .one-event #camp-event,
  .one-event #faire-event {
    border-right: 1px solid @background;
    padding-top: 15px;
  }




  
  div#shed-heading {
    padding-left: 2px;
    padding-right: 0;
  }

  #shed-heading h5 {
    font-size: @font-bold-header-size-S; 
    margin: 5px 0 0 3px;
  } 

  div#shed-tag {
    padding-left: 2px;
    padding-right: 0; 
  }

  #shed-tag h6 {
    font-size: 12px;
    margin-bottom: 0;
    vertical-align: middle;
  } 

  #shed-link {
    maring-left: 5px;
    padding-left: 3px;
  }

  #shed-link h6 {
    margin-left: 5px;
    margin-right: 5px;
    padding: 0;
  }

  #buy-link {
    display: none;
  }

  .link-arrow {
    margin-left: 1px;
    vertical-align: top;
  }

  .ad-unit {
    display: none; 
  }

}


/* Bootstrap MED */

@media screen and (min-width: 992px) and (max-width: 1199px) {

  /* Super Leaderboard DOES display on iPad horizontal */
  .ad-unit {
  }

  /* Leaderboard DOES NOT display on iPad horizontal */
  .ad-lb-unit {
    display: none;
  }




  .events-panel table {
    margin: 0 10px 0 20px;
  }

  .one-event h4 {
    color: #4A4A4A;
    font-size: 17px;
    font-weight: 300;
    margin: 0 15px 0 0;
  }

  .one-event h5 {
    color: #4A4A4A;    
    font-size: 12px;
    font-weight: 300;    
    margin-top: 12px;
