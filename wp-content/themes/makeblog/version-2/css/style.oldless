  } 

  #shed-link h6 {
    font-size: 12px;
  } 

  .link-arrow {
    margin-left: 5px;
    vertical-align: top;
  }

  #buy-link {
    display: none;
  } 
}



/* Bootstrap LARGE */

@media screen and (min-width: 1200px) {

  .events-panel table {
    margin: 0 10px 0 20px;
  }

  .one-event h4 {
    color: #4A4A4A;
    font-size: 17px;
    font-weight: 300;
    margin: 0 15px 0 0;
  }

  .one-event h5 {
    color: #4A4A4A;    
    font-size: 12px;
    font-weight: 300;    
    margin-top: 12px;
